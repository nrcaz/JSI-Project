<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <!--Meta pour card -->
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"> 
        <meta name="viewport" content="width=device-width, initial-scale=1.0"> 
        <meta name="description" content="Original Hover Effects with CSS3" />
        <meta name="keywords" content="css3, transitions, thumbnail, animation, hover, effect, description, caption" />
        <meta name="author" content="Alessio Atzeni for Codrops" />

        <title>{% block title %}JSI-Partners!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link href="{{ absolute_url(asset('assets/css/lux.css')) }} " rel="stylesheet">
        {# <link href="https://bootswatch.com/4/sketchy/bootstrap.min.css" rel="stylesheet"> #}
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/css/style.css')) }} ">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/css/nicho.css')) }} ">
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/css/flo.css')) }} ">
        <!--CSS Style card -->
        <link rel="shortcut icon" href="../favicon.ico"> 
        <link rel="stylesheet" type="text/css" href="{{absolute_url(asset('assets/css/css-card/demo.css')) }}" />
        <link rel="stylesheet" type="text/css" href="{{ absolute_url(asset('assets/css/css-card/style_common.css')) }}" />
        <!--card animation CSS -->
        <link rel="stylesheet" type="text/css" href="{{ absolute_url(asset('assets/css/css-card/style-card-all.css')) }}" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.0/animate.min.css">
       
        <link href='http://fonts.googleapis.com/css?family=Oswald' rel='stylesheet' type='text/css' />
    </head>
    <body>
        <header class="bg-light">
            <nav class="navbar navbar-expand-lg navbar-light">
        {# <header class="bg-dark"> #}
            {# <nav class="navbar navbar-expand-lg navbar-dark"> #}
                <a class="navbar-brand" href="{{ url("accueil") }}"><img src="{{ asset('assets/img/logo.png') }}" alt=""></a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
                    aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                    <div class="navbar-nav">
                        <a class="nav-item nav-link" href="{{ url("recherche") }}">Rechercher un bien</a>
                        <a class="nav-item nav-link" href="#services">Services</a>
                        <a class="nav-item nav-link" href="#commercialisation">Commercialisation</a>
                        <a class="nav-item nav-link" href="#contact_home">Contactez-nous</a>
                        <a class="nav-item nav-link" href="{{ url("back_office_accueil") }}">Backoffice</a>
                    </div>
                </div>
            </nav>
        </header>
        
        {% block body %}{% endblock %}
        
        <footer>
            <nav>
                <a class="nav-link" href="#references">Nos references</a>
                <a class="nav-link" href="{{ url("recrutement") }}">Recrutement</a>
            </nav>
            
            <div id="references">
                <img src="assets/img/logo.png" alt="">
                <img src="assets/img/Linkedin.png" alt="">
                <img src="assets/img/google-plus.png" alt="">
            </div>
            <nav class="mediaIcon">
                <a href="lien linkedin"><i class="fab fa-linkedin"></i></a>
                <a href="lien google plus"><i class="fab fa-google-plus"></i></a>
            </nav>
        </footer>
        <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>
        <script src="{{ absolute_url(asset('assets/js/parallax.js')) }}"></script>
        <script src="{{ absolute_url(asset('assets/js/nicho.js')) }}"></script>
        <script src="{{ absolute_url(asset('assets/js/flo.js')) }}"></script>
        <script src="{{ absolute_url(asset('assets/js/main.js')) }}"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
